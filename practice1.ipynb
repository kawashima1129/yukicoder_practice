    "import time\n",
    "\n",
    "x = input('> ')\n",
    "\n",
    "start = time.time()\n",
    "\n",
    "data_str = x.split()\n",
    "a = list(map(int, data_str))\n",
    "r = (a[1]-a[2])/(a[0]-a[1])\n",
    "d = a[1] - r * a[0]\n",
    "output = r * a[2] + d\n",
    "print(output)\n",
    "\n",
    "elapsed_time = time.time() - start\n",
    "print (\"elapsed_time:{0}\".format(elapsed_time) + \"[sec]\")"
   ]

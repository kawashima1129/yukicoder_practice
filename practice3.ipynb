    "x = []\n",
    "for i in range(5):\n",
    "    x.append(int(input('> ')))\n",
    "X_fee = x[0] * x[4]\n",
    "\n",
    "if x[4] <= x[2]:\n",
    "    Y_fee = x[1]\n",
    "else:\n",
    "    Y_fee = x[1] + x[3] * (x[4] - x[2])\n",
    "fee = min(X_fee, Y_fee)\n",
    "print(fee)"
